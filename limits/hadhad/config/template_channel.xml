<!DOCTYPE Channel SYSTEM 'HistFactorySchema.dtd'>

<Channel Name="hh_%(category)s_%(mass)d" >
    
    <Data HistoName="Data" InputFile="./data/%(category)s.root" />
    
    <StatErrorConfig RelErrorThreshold="0.05" ConstraintType="Poisson"/>

    <Sample Name="Signal_%(mass)d_ggf"
        InputFile="./data/%(category)s.root"
        HistoName="Signal_%(mass)d_ggf" NormalizeByTheory="True" >
        <NormFactor Name="SigXsecOverSM" Val="0" Low="0." High="40." Const="True" />
        <StatError Activate="False" />
        <HistoSys Name="JES_TES"
            HistoNameHigh="Signal_%(mass)d_ggf_JES_UP_TES_UP"
            HistoNameLow="Signal_%(mass)d_ggf_JES_DOWN_TES_DOWN" />
        <HistoSys Name="JER"
            HistoNameHigh="Signal_%(mass)d_ggf_JER_UP"
            HistoNameLow="Signal_%(mass)d_ggf" />
        <!--<OverallSys Name="T_EFF" High="1.08" Low="0.92"/>
        <OverallSys Name="T_TRIG" High="1.038" Low="0.958"/>-->
        <OverallSys Name="pdf_gg" High="1.079" Low="0.923"/>
        <OverallSys Name="QCDscale_ggH1in" High="1.133" Low="0.914"/>
    </Sample>
    
